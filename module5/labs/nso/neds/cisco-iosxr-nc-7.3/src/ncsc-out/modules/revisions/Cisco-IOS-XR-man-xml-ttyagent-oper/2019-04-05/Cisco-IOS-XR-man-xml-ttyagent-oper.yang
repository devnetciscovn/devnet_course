  grouping ncs_template_SESSION-TABLE {
    description
      "Common node of default, tty, ssl";
    container sessions {
      description
        "sessions information";
      list session {
        // NCS patched for predictable order
        ordered-by user;
        key "session-id";
        description
          "xml sessions information";
        leaf session-id {
          type string;
          description
            "Session Id";
        }
        uses ncs_template_XR-XML-SESSION-INFO;
      }
    }
  }

  grouping ncs_rpc_notif_SESSION-TABLE {
    description
      "Common node of default, tty, ssl";
    container sessions {
      description
        "sessions information";
      list session {
        key "session-id";
        description
          "xml sessions information";
        leaf session-id {
          type uint32 {
            range "1..4294967294";
          }
          description
            "Session Id";
        }
        uses ncs_rpc_notif_XR-XML-SESSION-INFO;
      }
    }
  }

  grouping live_ncs_SESSION-TABLE {
