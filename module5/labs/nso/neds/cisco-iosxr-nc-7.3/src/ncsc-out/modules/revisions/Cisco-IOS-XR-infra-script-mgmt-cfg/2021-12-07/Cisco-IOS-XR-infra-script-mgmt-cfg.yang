        uses live_ncs_SCRIPT-TABLE;
      }
      container process-script {
        description
          "Process script";
        uses live_ncs_SCRIPT-TABLE;
      }
    }
  }
  augment "/ncs:devices/ncs:template/ncs:ned-id/ncs:config" {
