          }
        }
        leaf script-name {
          type xr:Cisco-ios-xr-string;
          description
            "Script name";
        }
      }
    }
  }

  augment "/ncs:devices/ncs:device/ncs:config" {
    container scripts {
      tailf:ncs-device-type "netconf";
      tailf:display-when "not(../../ncs:module) or ../../ncs:module[ncs:name='Cisco-IOS-XR-infra-script-mgmt-cfg']";
      description
        "Script-mgmt configuration data";
      container exec-script {
        description
          "Exec script";
        uses SCRIPT-TABLE;
      }
      container process-script {
        description
          "Process script";
        uses SCRIPT-TABLE;
      }
    }
  }
  augment "/ncs:devices/ncs:device/ncs:live-status" {
